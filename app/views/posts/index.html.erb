<%= button_to "Log Out", destroy_user_session_path, method: :delete, class: "btn cyan" %>
<h2>Welcome <%= current_user.profile_name %></h2>
<hr />
<%= link_to "Add a New Post", new_post_path, class: "btn purple" %>
<h3>Your Posts</h3>
<ul>
  <% @posts.each do |post| %>
    <li>
      <%= post.title %> - <%= post.body %>
    </li>
    <br>
  <% end %>
</ul>
